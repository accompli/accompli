{
    "name": "accompli/accompli",
    "type": "library",
    "description": "An easy to use and extendable deployment tool for PHP projects.",
    "keywords": [
        "Accompli", "Deployment",
        "SSH", "SFTP", "FTP", "FTPS"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Niels Nijens",
            "email": "nijens.niels@gmail.com",
            "role": "Lead developer"
        }
    ],
    "support": {
        "docs": "https://accompli.readme.io"
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "accompli/chrono": "^0.3.0",
        "composer/semver": "^1.2",
        "justinrainbow/json-schema": "^1.3 || ^2.0",
        "niels-nijens/protocol-stream": "^1.0",
        "niels-nijens/utilities": "^3.2",
        "phpseclib/phpseclib": "^2.0",
        "psr/log": "^1.0",
        "seld/jsonlint": "^1.3",
        "symfony/config": "^2.7 || ^3.0",
        "symfony/console": "^2.7 || ^3.0",
        "symfony/dependency-injection": "^2.7 || ^3.0",
        "symfony/event-dispatcher": "^2.7 || ^3.0",
        "symfony/filesystem": "^2.7 || ^3.0",
        "symfony/process": "^2.7 || ^3.0",
        "symfony/yaml": "^2.7 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Accompli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accompli\\Test\\": "tests/"
        }
    },
    "bin": ["bin/accompli"]
}
